  <script src="roundabout.js" type="text/javascript"></script>
  <script src="skel.min.js" type="text/javascript"></script>
  <script src="init.js" type="text/javascript"></script>
  <script src="roundabout_shapes.js" type="text/javascript"></script>
  <script src="jquery.easing.1.2.js" type="text/javascript"></script>
  <script type="text/javascript" src="script.js"></script>
  <%= csrf_meta_tags %>
